package com.cts.jf.service;

public class NumberSeriesService implements Runnable {

	private int start;
	private int end;
	
	public NumberSeriesService(int start, int end) {
		super();
		this.start = start;
		this.end = end;
	}

	@Override
	public void run() {
		for(int i=start;i<=end;i++) {
			System.out.println(i);
		}
	}
	
}
