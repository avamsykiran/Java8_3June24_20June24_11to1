package com.cts.jf.service;

import com.cts.jf.models.BankAccount;

public class BankingService {

	public void deposit(BankAccount acc,double amount) {
		if(acc!=null) {
			acc.setCurrentBalance(acc.getCurrentBalance()+amount);
		}
	}
	
	public void withdraw(BankAccount acc,double amount) {
		if(acc!=null) {
			acc.setCurrentBalance(acc.getCurrentBalance()-amount);
		}
	}
}
